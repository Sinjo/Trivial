<resources>
    <string name="app_name">Trivial</string>
    
    <!-- Displayed on the button which begins the quiz when clicked -->
    <string name="start_quiz">Start Quiz</string>
    
    <!-- Strings displayed when errors are encountered -->
    
    <!-- IOException while loading the questions from the asset files -->
    <string name="question_reading_ioexception">A problem was encountered while loading the questions, and has been logged. Please send the developer a bug report including log output.</string>
	<!-- Number of strings that could not be parsed -->
	<plurals name="question_reading_parse_fail_number">
        <item quantity="one">1 question could not be parsed, and has been logged. Please send the developer a bug report including log output.</item>
        <item quantity="other">%d questions could not be parsed, and have been logged. Please send the developer a bug report including log output.</item>
    </plurals>

</resources>
